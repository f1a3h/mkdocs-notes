{
  "database": "Zotero",
  "noteImportFolder": "Research/Papers",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Bibliography",
      "format": "formatted-bibliography",
      "cslStyle": "apa"
    }
  ],
  "exportFormats": [
    {
      "name": "Import Zotero Note",
      "outputPathTemplate": "Research/Papers/{{citekey}}.md",
      "imageOutputPathTemplate": "Assets/{{citekey}}/",
      "imageBaseNameTemplate": "image",
      "cslStyle": "apa",
      "templatePath": "_ignore/Templates/zotero_integration.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note"
}