site_name: f1a3h's notebook
site_author: f1a3h
# site_url: 
site_description: A simple digital garden.

copyright: Copyright &copy; 2024 <a href="https://github.com/f1a3h" target="_blank" rel="noopener">f1a3h</a>

theme:
  name: material
  language: en
  icon:
    logo: material/pac-man
  features:
    - content.code.annotate
    - content.code.copy
    - navigation.tracking
    - navigation.tabs
    - navigation.indexes
    - navigation.top
    - search.highlight
    - search.share
  custom_dir: overrides
  font:
    text: LXGW WenKai Screen GB Screen
    code: JetBrains Mono
  palette:
    - scheme: default

extra_css:
  - assets/css/extra.css
  - https://jsd.cdn.zzko.cn/npm/katex@0.16.4/dist/katex.min.css
  - https://gcore.jsdelivr.net/npm/lxgw-wenkai-screen-webfont@1.1.0/style.css
  - https://gcore.jsdelivr.net/npm/lxgw-wenkai-webfont@1.1.0/style.css
  - https://fonts.googleapis.com/css2?family=Crimson+Pro&family=JetBrains+Mono&family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&family=Merriweather+Sans&display=swap

extra_javascript:
  - assets/js/extra.js
  - assets/js/katex.js
  - https://jsd.cdn.zzko.cn/npm/katex@0.16.4/dist/katex.min.js

nav:
  - Home: index.md
